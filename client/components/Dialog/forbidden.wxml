<!--components/Dialog/forbidden.wxml-->
<!-- <text>components/Dialog/forbidden.wxml</text> -->
<view class="dialog-wrap {{dialogVisible?'':'fadeout'}}">
  <view class="mask" bindtap="dialogTapHandle"></view>
  <view class="dialog">
    <image src="/assets/images/popup_disable.png" mode="aspectFill"></image>
    <view class="tit">{{forbiddenObj.text1}}</view>
    <view class="help">{{forbiddenObj.text2}}</view>
    <navigator class="btn btn-confirm" open-type="exit" target="miniProgram">确定</navigator>
  </view>
</view>