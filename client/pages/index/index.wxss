/* 无名片信息 */
.container-welcome {}

.compatibleIphoneXEl {
  padding-top: env(safe-area-inset-bottom);
  background-color: #3c79ff;
}

.container-welcome .section1 {}

.container-welcome .section1 image {
  width: 100vw;
  height: 987rpx;
}

.container-welcome .section1 .card {
  margin-top: env(safe-area-inset-bottom);
  position: absolute;
  top: 283rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 630rpx;
  height: 704rpx;
  /* background: rgba(255, 255, 255, 0.39); */
}

.container-welcome .section1 .card .l1 {
  margin-top: 30rpx;
  text-align: center;
}

.container-welcome .section1 .card .l1 view {
  font-size: 33rpx;
  font-weight: 500;
  color: #555555;
  letter-spacing: 2rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.container-welcome .section1 .card .l1 view::after,
.container-welcome .section1 .card .l1 view::before {
  content: '';
  width: 14rpx;
  height: 12rpx;
  /* background: rgba(73, 128, 249, 0.39); */
  background: #4980F9;
}

.container-welcome .section1 .card .l1 view::after {
  margin-left: 15rpx;
}

.container-welcome .section1 .card .l1 view::before {
  margin-right: 15rpx;
}

.container-welcome .section1 .card .l2 {
  text-align: center;
  margin-top: 18rpx;
  font-size: 22rpx;
  color: #9F9F9F;
  letter-spacing: 2rpx;
}

.container-welcome .section1 .card .l3 swiper {
  margin: 22rpx auto 0;
  width: 570rpx;
  height: 549rpx;
  background: rgba(242, 242, 242, 0.39);
  opacity: 1;
  border-radius: 8rpx;
}

.container-welcome .section2 {
  margin-top: 44rpx;
}

.container-welcome .section2 .btn-create {
  margin: 0 auto;
  width: 694rpx;
  height: 86rpx;
  /* background: rgba(73, 128, 249, 0.39); */
  background: var(--primary-color);
  box-shadow: 0rpx 6rpx 24rpx rgba(0, 57, 181, 0.3);
  opacity: 1;
  border-radius: 8rpx;
  font-size: 32rpx;
  font-weight: 500;
  line-height: 86rpx;
  color: #FFFFFF;
  letter-spacing: 2rpx;
  text-align: center;
}

.container-welcome .section3 {
  margin-top: 34rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 57rpx;
}

.container-welcome .section3 .radio_box {
  width: 32rpx;
  height: 32rpx;
  background: rgba(255, 255, 255, 0.39);
  border: 1rpx solid #707070;
  border-radius: 50%;
  position: relative;
}

.container-welcome .section3 .radio {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}

.container-welcome .section3 .radio_img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.container-welcome .section3 .agree {
  margin-left: 20rpx;
  font-size: 26rpx;
  color: #CCCCCC;
}

.container-welcome .section3 .agree .agreement {
  font-size: 26rpx;
  color: #A0A0A0;
}

/* 名片信息 */

.container .image_bg {
  width: 100vw;
  height: 394rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.container .content {
  background: #FFFFFF;
}

.container-index .section1 {
  margin: 20rpx auto 0;
  width: 694rpx;
  background: rgba(255, 255, 255, 0.39);
  box-shadow: 0rpx 6rpx 16rpx rgba(0, 0, 0, 0.12);
  opacity: 1;
  border-radius: 10rpx;
  min-height: 400rpx;
}

.container-index .section2 {
  margin-top: 44rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 34rpx 0 28rpx;
  font-size: 24rpx;
}

.container-index .section2 .left,
.container-index .section2 .right {
  display: flex;
  align-items: center;
}

.container-index .section2 .left .avatar_box {}

.container-index .section2 .left .avatar {
  width: 48rpx;
  height: 48rpx;
  background: rgba(0, 0, 0, 0);
  border: 2rpx solid #FFFFFF;
  border-radius: 50%;
  margin-left: -8rpx;
}

.container-index .section2 .right .icon_liked {
  width: 26rpx;
  height: 26rpx;
}

.container-index .section2 .left .text {
  margin-left: 12rpx;
  font-size: 24rpx;
  color: #747888;
}

.container-index .section2 .right .text {
  margin-left: 10rpx;
  font-size: 26rpx;
  color: #747888;
}

.container-index .section3 {
  margin-top: 30rpx;
}

.section3 {
  display: flex;
  padding-bottom: 30rpx;
}

.section3 .btn-share,
.section3 .btn-reply {
  width: 323rpx;
  height: 74rpx;
  background: rgba(255, 255, 255, 0.39);
  border: 2rpx solid #4980F9;
  border-radius: 8rpx;
}

.section3 .btn-share {
  color: #4980F9;
}

.section3 .btn-reply {
  background: #4980F9;
  color: #FFFFFF;
}

.section3 .btn-share image {
  width: 34rpx;
  height: 34rpx;
}

.section3 .btn-reply image {
  width: 36rpx;
  height: 36rpx;
}

.section3 .btn text {
  margin-left: 10rpx;
  font-size: 28rpx;
  line-height: 74rpx;
}

.section4 {
  padding: 24rpx 28rpx;
  background: #F0F3F7;
}

.section4 .item-box {
  /* width: calc(100vw - 56rpx); */
  display: flex;
  white-space: nowrap;
  overflow: scroll;
}

.section4 .item {
  /* height: 120rpx; */
  background: rgba(255, 255, 255, 0.39);
  border: 1rpx solid #EDF0F5;
  border-radius: 6rpx;
  margin-right: 16rpx;
  padding: 24rpx 24rpx 30rpx 24rpx;
}

.section4 .item:last-child {
  margin-right: 116rpx;
}

.section4 .item .row1 {
  display: flex;
}

.section4 .item .row1 image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}

.section4 .item .row2 {
  margin-top: 16rpx;
  font-size: 25rpx;
  color: #888888;
}

.section5 {}

.section5 .tabs-nav {
  display: flex;
  position: relative;
  background: #ffffff;
  white-space: nowrap;
}

.section5 .tabs-nav .tab {
  padding: 22rpx 0;
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  position: relative;
  font-size: 28rpx;
  color: #999999;
}

.section5 .tabs-nav .tab.active {
  color: #333333;
}

.section5 .tabs-nav .tab>view {
  display: inline-block;
}

.section5 .tabs-nav .tabs-line {
  box-sizing: border-box;
  position: absolute;
  bottom: 0;
  left: 0;
  /* z-index: 1; */
  transition-duration: .3s;
  width: 55rpx;
  height: 8rpx;
  border-radius: 10rpx;
  background: #333333;
}

.cont {
  overflow: auto;
}

.cont .no-data {
  text-align: center;
}

.cont .no-data image {
  margin-top: 60rpx;
  width: 440rpx;
  height: 332rpx;
}

.cont .no-data .text {
  margin-top: 40rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #BABEC2;
}